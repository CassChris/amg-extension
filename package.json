{
  "name": "amg-extension",
  "displayName": "amg-extension",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/CassChris/amg-extension"
  },
  "engines": {
    "vscode": "^1.73.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "amg-extension.helloWorld",
      "title": "Hello World"
    }],
    "snippets": [
      {
        "language": "vue",
        "path": "./snippets/vue-snippets.code-snippets"
      },
      {
        "language": "php",
        "path": "./snippets/php-snippets.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/js-snippets.code-snippets"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
